{
  "general": {
    "defaultApprovalMode": "auto_edit"
  },
  "tools": {
    "sandbox": "docker"
  },
  "telemetry": {
    "enabled": true,
    "logFile": ".gemini/telemetry.json"
  },
  "hooks": {
    "BeforeTool": [
      ".gemini/hooks/block-secrets.sh"
    ],
    "AfterTool": [
      ".gemini/hooks/audit-logger.sh"
    ]
  },
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": [
        "github-mcp"
      ]
    },
    "vercel": {
      "command": "npx",
      "args": [
        "vercel-mcp"
      ]
    },
    "figma": {
      "command": "npx",
      "args": [
        "figma-mcp"
      ]
    },
    "adb": {
      "command": "npx",
      "args": [
        "adb-mcp"
      ]
    },
    "gradle": {
      "command": "npx",
      "args": [
        "gradle-mcp"
      ]
    },
    "unity": {
      "command": "npx",
      "args": [
        "unity-mcp"
      ]
    },
    "asset": {
      "command": "npx",
      "args": [
        "asset-mcp"
      ]
    },
    "huggingface": {
      "command": "npx",
      "args": [
        "hf-mcp"
      ]
    },
    "arxiv": {
      "command": "npx",
      "args": [
        "arxiv-mcp"
      ]
    },
    "dovetail": {
      "command": "npx",
      "args": [
